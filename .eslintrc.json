{
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import", "require"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "linebreak-style": "off",
    "react/prop-types": "off",
    "global-require": "off",
    "react/jsx-no-target-blank": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/accessible-emoji": "off"
  },
  "env": {
    "browser": true
  }
}
